{
	"name": "@thingco/graphviz",
	"description": "A small data visualisation library for ThingCo front ends. NOT a renderer, this just takes data and processes it to a form usable by a renderer",
	"version": "1.0.0-alpha.1",
	"main": "lib/index.js",
	"module": "lib/index.module.js",
	"types": "lib/index.d.ts",
	"public": true,
	"scripts": {
		"clean": "yarn global:clean lib node_modules",
		"bundle": "yarn microbundle",
		"develop": "yarn clean && yarn microbundle watch",
		"build": "yarn clean && yarn bundle",
		"lint": "yarn global:lint"
	},
	"devDependencies": {
		"microbundle": "^0.13.0",
		"typescript": "^4.2.3"
	}
}
